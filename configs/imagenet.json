{
  "n_epochs": 5000,
  "n_save_steps": 500,
  "dataset": {
    "type": "cifar",
    "data_path": "../../data/imagenet",
    "label_a": "bird",
    "label_b": "ship",
    "n_tracks": 5000
  },
  "loading": {
    "batch_size": 16,
    "shuffle": true
  },
  "model": {
    "gen": {
      "input_nc": 3,
      "output_nc": 3,
      "n_filters": 64,
      "num_downs": 5,
      "dropout": false,
      "norm_layer": "bn",
      "sn": true,
      "sa": true
    },
    "dis": {
      "input_nc": 3,
      "n_filters": 64,
      "num_downs": 4,
      "norm_layer": "none",
      "sn": true,
      "sa": true
    },
    "siam": {
      "input_nc": 3,
      "n_filters": 64,
      "num_downs": 4,
      "latent_dim": 1000,
      "norm_layer": "none"
    },
    "lr_gen": 1e-4,
    "lr_dis": 4e-4,
    "margin": 10,
    "lambda_adv": 1,
    "lambda_travel": 10,
    "lambda_margin": 10,
    "lambda_gp": 0,
    "type": "hinge"
  }
}
